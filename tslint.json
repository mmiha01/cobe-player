{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [false],
        "quotemark": [
            true,
            "single",
            "avoid-template",
            "avoid-escape"
          ],
          "eofline": false,
          "trailing-comma": false,
          "interface-name": false,
          "member-access": false,
          "no-var-requires": false

    },
    "rulesDirectory": []
}